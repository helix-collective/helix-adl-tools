{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/auth0-js": "^8.11.7",
    "@types/base64-js": "^1.2.5",
    "@types/classnames": "^2.2.6",
    "@types/react-google-recaptcha": "^0.10.0",
    "auth0-js": "^9.8.2",
    "autoprefixer": "7.1.6",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "^22.1.3",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "4.2.1",
    "react-dom": "^16.2.0",
    "react-google-recaptcha": "^1.0.5",
    "react-markdown": "^4.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^22.1.3",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.11.0",
    "tslint-react": "^3.2.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "3.1.11",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "init-dummy-data": "ts-node -O '{\"module\": \"commonjs\"}' src/tools/init-dummy-data.ts",
    "lint": "tslint --format verbose --project .",
    "lint:fix": "tslint --format verbose --fix --project .",
    "pretty": "prettier --write $(find src -iname '*.ts' -o -iname '*.tsx' | grep -v genfiles | grep -v 'adl-gen')",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom --testPathIgnorePatterns integration.test",
    "test:all": "node scripts/test.js --env=jsdom"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/node-fetch": "^2.1.2",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "file-loader": "^2.0.0",
    "prettier": "^1.13.7",
    "puppeteer": "^1.6.0",
    "react-test-renderer": "^16.4.1",
    "ts-node": "^7.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^2.9.2"
  },
  "resolutions": {
    "@types/react": "^16.0.38"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/setup-jest.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
