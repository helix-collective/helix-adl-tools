{
  "name": "hx-adl",
  "version": "0.1",
  "description": "helix specific adl processors",
  "main": "index.js",
  "repository": "",
  "author": "Tim Docker",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "base64-js": "^1.3.0",
    "change-case": "^3.1.0",
    "commander": "^2.20.0",
    "fs-extra": "^9.0.1",
    "mkdirp": "^0.5.1",
    "mustache": "^4.0.0",
    "tmp": "^0.1.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.test\\.ts$"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/change-case": "^2.3.1",
    "@types/fs-extra": "^9.0.2",
    "@types/jest": "23.3.13",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash": "^4.14.110",
    "@types/mkdirp": "^0.5.2",
    "@types/mustache": "^4.0.1",
    "@types/node": "12",
    "@types/tmp": "^0.1.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.19",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0"
  },
  "scripts": {
    "lint": "tslint --format verbose --project .",
    "lint:fix": "tslint --format verbose --fix --project .",
    "prettier": "prettier --write $(find . -type d -name node_modules -prune -o  -type d -name adl-gen -prune -o  -iname '*.ts*' -not -exec grep -q [@]generated '{}' \\; -print)",
    "clean": "rm -rf build",
    "build": "yarn && yarn clean && yarn tsc",
    "typecheck": "yarn && yarn tsc --noEmit",
    "test": "jest",
    "server": "node build/server-main.js"
  }
}
